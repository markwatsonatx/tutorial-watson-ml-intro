{
  "env": {
    "vars": [
      {
        "name": "WML_SERVICE_PATH",
        "defaultValue": "https://ibm-watson-ml.mybluemix.net"
      },
      { "name": "WML_USERNAME" },
      { "name": "WML_PASSWORD" },
      { "name": "WML_INSTANCE_ID" }
    ]
  },
  "editor" : {
    "hide": false,
    "srcDir": "src" 
  },
  "proxy": {
    "ports": [
      {
        "port": 38889,
        "hide": false,
        "name": "Notebook",
        "path": "/notebooks/HousePrices.ipynb"
      },
      {
        "port": 33000,
        "hide": false,
        "name": "Web App",
        "path": "/"
      }
    ]
  }
}
